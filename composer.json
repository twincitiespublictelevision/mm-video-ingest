{
  "name": "twincitiespublictelevision/mm-ingest",
  "description": "A video ingest application for PBS Media Manager",
  "license": "proprietary",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "laravel/lumen-framework": "5.4.*",
    "vlucas/phpdotenv": "~2.2",
    "doctrine/dbal": "^2.5",
    "twincitiespublictelevision/PBS_Media_Manager_Client": "dev-composed",
    "mikey179/vfsStream": "^1.6"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/twincitiespublictelevision/PBS_Media_Manager_Client.git"
    }
  ]
}
